<Page x:Class="MAF.TextAnalytics.CheckPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:MAF.TextAnalytics"
      mc:Ignorable="d" Height="248.489" Width="561.401"
      Title="Feldolgozott fálok ellenőrzése">

    <Grid Margin="0,-19,0,0">
        <Grid.Background>
            <RadialGradientBrush>
                <GradientStop Color="White" Offset="1"/>
                <GradientStop Color="#FF79BEC1"/>
            </RadialGradientBrush>
        </Grid.Background>
        <Label Content="Fájl:" HorizontalAlignment="Left" Margin="18,65,0,0" VerticalAlignment="Top" Width="39"/>
        <TextBox x:Name="tbFile" HorizontalAlignment="Left" Height="26" Margin="75,65,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="438" RenderTransformOrigin="0.276,0.739"/>
        <Button Content="OK" HorizontalAlignment="Left" Margin="126,179,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click_1"/>
        <Button Content="Mégsem" HorizontalAlignment="Left" Margin="380,179,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click"/>
    </Grid>
</Page>
